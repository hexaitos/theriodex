<% if !session[:font] then %>
	<link rel="preload" as="style" href="/css/fonts/default.css">
	<link rel="stylesheet" href="/css/fonts/default.css">
<% else %>
	<link rel="preload" as="style" href="/css/fonts/<%= session[:font] %>.css">
	<link rel="stylesheet" href="/css/fonts/<%= session[:font] %>.css">
<% end %>
