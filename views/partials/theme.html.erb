<% if !session[:theme] or session[:theme] == "default" then %>
	<link rel="preload" as="style" href="/css/themes/default.css">
	<link rel="stylesheet" href="/css/themes/default.css">
<% else %>
	<link rel="preload" as="style" href="/css/themes/<%= session[:theme] %>">
	<link rel="stylesheet" href="/css/themes/<%= session[:theme] %>">
<% end %>
